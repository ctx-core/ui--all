{
	"name": "relementjs",
	"version": "0.5.0",
	"description": "reactive elements...browser & server rendering...tiny, fast, scales large, scales small",
	"keywords": [
		"reactive",
		"ui",
		"ui",
		"memos",
		"signals",
		"rmemo",
		"ctx-core"
	],
	"homepage": "https://github.com/relementjs/any#readme",
	"bugs": {
		"url": "https://github.com/relementjs/any/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/relementjs/any.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./browser",
		"./html",
		"./isomorphic",
		"./mathml",
		"./server",
		"./svg"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./browser": "./browser/index.js",
		"./html": "./html/index.js",
		"./isomorphic": "./isomorphic/index.js",
		"./mathml": "./mathml/index.js",
		"./server": "./server/index.js",
		"./svg": "./svg/index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"size-limit": "^11.0.0",
		"test": "pnpm run /^test:/",
		"test:size": "size-limit",
		"test:type": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"ctx-core": "^4.6.0"
	},
	"devDependencies": {
		"@size-limit/preset-small-lib": "^11.0.0",
		"@types/jsdom": "^21.1.6",
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"jsdom": "^22.1.0",
		"size-limit": "^11.0.0",
		"tsx": "^4.5.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false,
	"size-limit": [
		{
			"name": "isomorphic browser",
			"import": {
				"": "{ rel__use }",
				"./browser": "{ browser__rel }"
			},
			"limit": "949 B"
		},
		{
			"name": "isomorphic server",
			"import": {
				"": "{ rel__use }",
				"./server": "{ server__rel }"
			},
			"limit": "748 B"
		},
		{
			"name": "isomorphic helpers",
			"import": {
				"": "{ rel__use, attach, bind_, tags, tagsNS }"
			},
			"limit": "163 B"
		},
		{
			"name": "browser-only minimal",
			"import": {
				"./browser": "{ tags }"
			},
			"limit": "734 B"
		},
		{
			"name": "browser-only hydrate + helpers",
			"import": {
				"./browser": "{ tags, tagsNS, hydrate, bind_ }"
			},
			"limit": "781 B"
		},
		{
			"name": "browser-only hydrate + helpers + rmemo",
			"import": {
				"./browser": "{ tags, tagsNS, hydrate, bind_, memo_, sig_ }"
			},
			"limit": "799 B"
		},
		{
			"name": "browser-only hydrate + helpers + rmemo + ctx",
			"import": {
				"./browser": "{ tags, tagsNS, hydrate, bind_, memo_, sig_, be_, ctx_ }"
			},
			"limit": "959 B"
		},
		{
			"name": "isomorphic browser helpers + hydrate + helpers + rmemo + ctx",
			"import": {
				"": "{ rel__use, bind_, tags, hydrate, memo_, sig_, be_, ctx_ }",
				"./browser": "{ browser__rel }"
			},
			"limit": "1123 B"
		},
		{
			"name": "server-only minimal",
			"import": {
				"./server": "{ tags }"
			},
			"limit": "515 B"
		},
		{
			"name": "server-only render doc + helpers",
			"import": {
				"./server": "{ tags, tagsNS, doc_html_, bind_, }"
			},
			"limit": "555 B"
		},
		{
			"name": "server-only render doc + helpers + rmemo",
			"import": {
				"./server": "{ tags, tagsNS, doc_html_, bind_, memo_, sig_ }"
			},
			"limit": "891 B"
		},
		{
			"name": "server-only render doc + helpers + rmemo + ctx",
			"import": {
				"./server": "{ tags, tagsNS, doc_html_, bind_, memo_, sig_, be_, ctx_ }"
			},
			"limit": "1052 B"
		},
		{
			"name": "isomorphic browser helpers + hydrate + helpers + rmemo + ctx",
			"import": {
				"": "{ rel__use, bind_, tags, hydrate, memo_, sig_, be_, ctx_ }",
				"./server": "{ server__rel }"
			},
			"limit": "1242 B"
		},
		{
			"name": "html: html_",
			"import": {
				"./html": "{ html_ }"
			},
			"limit": "34 B"
		},
		{
			"name": "mathml: math_",
			"import": {
				"./mathml": "{ math_ }"
			},
			"limit": "34 B"
		},
		{
			"name": "svg: svg_",
			"import": {
				"./svg": "{ svg_ }"
			},
			"limit": "33 B"
		}
	]
}

{
	"name": "relementjs",
	"version": "0.64.2",
	"description": "reactive elements...browser & server rendering...tiny, fast, scales large, scales small",
	"keywords": [
		"ui",
		"template",
		"rendering",
		"server-side rendering",
		"dom",
		"dom Generation",
		"dom Manipulation",
		"hydration",
		"vanilla",
		"ultra-lightweight",
		"minimalist",
		"scalable",
		"reactive",
		"memos",
		"signals",
		"rmemo",
		"ctx-core"
	],
	"homepage": "https://github.com/relementjs/relementjs#readme",
	"bugs": {
		"url": "https://github.com/relementjs/relementjs/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/relementjs/relementjs.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"any",
		"astro",
		"browser",
		"html",
		"isomorphic",
		"mathml",
		"server",
		"svg"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./any": "./any/index.js",
		"./astro": "./astro/Relement.astro",
		"./browser": "./browser/index.js",
		"./html": "./html/index.js",
		"./isomorphic": "./isomorphic/index.js",
		"./mathml": "./mathml/index.js",
		"./server": "./server/index.js",
		"./svg": "./svg/index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"size-limit": "^11.0.0",
		"test": "pnpm run /^test:/",
		"test:size": "size-limit",
		"test:type": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"ctx-core": "^5.38.1"
	},
	"devDependencies": {
		"@size-limit/preset-small-lib": "^11.0.2",
		"@types/jsdom": "^21.1.6",
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"jsdom": "^24.0.0",
		"size-limit": "^11.0.2",
		"tsx": "^4.7.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false,
	"size-limit": [
		{
			"name": "isomorphic browser",
			"import": {
				"": "{ relement__use }",
				"./browser": "{ browser__relement }"
			},
			"limit": "728 B"
		},
		{
			"name": "isomorphic browser + rmemo",
			"import": {
				"": "{ relement__use }",
				"./browser": "{ browser__relement, memo_, sig_ }"
			},
			"limit": "1058 B"
		},
		{
			"name": "isomorphic browser + rmemo + item_list_",
			"import": {
				"": "{ relement__use }",
				"./browser": "{ browser__relement, memo_, sig_ }",
				"./isomorphic": "{ item_list_ }"
			},
			"limit": "1224 B"
		},
		{
			"name": "isomorphic server",
			"import": {
				"": "{ relement__use }",
				"./server": "{ server__relement }"
			},
			"limit": "712 B"
		},
		{
			"name": "isomorphic server + rmemo",
			"import": {
				"": "{ relement__use }",
				"./server": "{ server__relement, memo_, sig_ }"
			},
			"limit": "1064 B"
		},
		{
			"name": "isomorphic helpers",
			"import": {
				"": "{ relement__use, attach, tags, tagsNS }"
			},
			"limit": "136 B"
		},
		{
			"name": "browser-only minimal",
			"import": {
				"./browser": "{ tags }"
			},
			"limit": "429 B"
		},
		{
			"name": "browser-only hydrate",
			"import": {
				"./browser": "{ tags, tagsNS, hydrate }"
			},
			"limit": "470 B"
		},
		{
			"name": "browser-only hy__bind",
			"import": {
				"./browser": "{ tags, tagsNS, hy__bind }"
			},
			"limit": "501 B"
		},
		{
			"name": "browser-only hydrate + rmemo",
			"import": {
				"./browser": "{ tags, tagsNS, hydrate, _, memo_, sig_ }"
			},
			"limit": "816 B"
		},
		{
			"name": "browser-only hy__bind + rmemo",
			"import": {
				"./browser": "{ tags, tagsNS, hy__bind, _, memo_, sig_ }"
			},
			"limit": "849 B"
		},
		{
			"name": "browser-only hydrate + rmemo + ctx",
			"import": {
				"./browser": "{ tags, tagsNS, hydrate, _, memo_, sig_, be_, ctx_ }"
			},
			"limit": "917 B"
		},
		{
			"name": "browser-only hy__bind + rmemo + ctx",
			"import": {
				"./browser": "{ tags, tagsNS, hy__bind, _, memo_, sig_, be_, ctx_ }"
			},
			"limit": "946 B"
		},
		{
			"name": "isomorphic browser hydrate + rmemo + ctx",
			"import": {
				"": "{ relement__use, tags, _, memo_, sig_, be_, ctx_ }",
				"./browser": "{ browser__relement, hydrate }"
			},
			"limit": "1212 B"
		},
		{
			"name": "isomorphic browser hy__bind + rmemo + ctx",
			"import": {
				"": "{ relement__use, tags, _, memo_, sig_, be_, ctx_ }",
				"./browser": "{ browser__relement, hy__bind }"
			},
			"limit": "1235 B"
		},
		{
			"name": "server-only minimal",
			"import": {
				"./server": "{ tags }"
			},
			"limit": "484 B"
		},
		{
			"name": "server-only render doc",
			"import": {
				"./server": "{ tags, tagsNS, doc_html_, }"
			},
			"limit": "518 B"
		},
		{
			"name": "server-only render doc + rmemo",
			"import": {
				"./server": "{ tags, tagsNS, doc_html_, _, memo_, sig_ }"
			},
			"limit": "878 B"
		},
		{
			"name": "server-only render doc + rmemo + ctx",
			"import": {
				"./server": "{ tags, tagsNS, doc_html_, _, memo_, sig_, be_, ctx_ }"
			},
			"limit": "982 B"
		},
		{
			"name": "isomorphic server helpers + doc_html_ + rmemo + ctx",
			"import": {
				"": "{ relement__use, tags, _, memo_, sig_, be_, ctx_ }",
				"./server": "{ server__relement, doc_html_ }"
			},
			"limit": "1200 B"
		},
		{
			"name": "html: html_",
			"import": {
				"./html": "{ html_ }"
			},
			"limit": "34 B"
		},
		{
			"name": "mathml: math_",
			"import": {
				"./mathml": "{ math_ }"
			},
			"limit": "34 B"
		},
		{
			"name": "svg: svg_",
			"import": {
				"./svg": "{ svg_ }"
			},
			"limit": "33 B"
		}
	]
}
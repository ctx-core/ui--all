{
	"name": "@ctx-core/ui--all",
	"version": "0.0.0",
	"description": "ui library targeting all environments",
	"keywords": [
		"ctx-core",
		"ui",
		"browser",
		"server"
	],
	"homepage": "https://github.com/ctx-core/ui--any#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/ui--any/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/ui--any.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./html",
		"./render"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./html": "./html/index.js",
		"./render": "./render/index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"size-limit": "^11.0.0",
		"test": "pnpm run /^test:/",
		"test:size": "size-limit",
		"test:type": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@ctx-core/ui--any": "^0.0.0",
		"@ctx-core/ui--browser": "^0.0.2",
		"@ctx-core/ui--server": "^0.1.1",
		"ctx-core": "^4.4.0"
	},
	"devDependencies": {
		"@size-limit/preset-small-lib": "^11.0.0",
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"size-limit": "^11.0.0",
		"tsx": "^4.3.0",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"sideEffects": false,
	"size-limit": [
		{
			"name": "render_api__use _ attach bind tags tagsNS",
			"import": {
				"": "{ render_api__use, _, attach, bind_, tags, tagsNS }"
			},
			"limit": "108 B"
		}
	]
}
